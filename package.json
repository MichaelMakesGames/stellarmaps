		"update-intl": "node ./scripts/update-intl",
		"postinstall": "patch-package"
	},
	"simple-git-hooks": {
		"pre-commit": "npm run knip && npm run svelte-check && npm run format:check && npm run lint",
		"commit-msg": "npm run commitlint"
	},
